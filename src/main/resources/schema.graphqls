type Query {
    users: [User]
    user(id: Int!): User
    cars: [Car]
    car(id: Int!): Car
    me: User
}

type User {
    id: ID!
    name: String!
    vehicles: [Car]
}

type Car {
    id: ID!
    make: String!
    model: String!
    color: String!
    owner: User
}

type Mutation {
    makeUser(id: Int!, name: String): User!
    deleteUser(id: Int!): Boolean
    createCar(id: Int!, make: String!, model: String!, color: String!): Car
    deleteCar(id: Int!): Boolean
}